{
  "kind": "collectionType",
  "collectionName": "chefs",
  "info": {
    "singularName": "chef",
    "pluralName": "chefs",
    "displayName": "Chef"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "rating": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "reviewCount": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "priceRange": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "enum": [
        "Budget",
        "Standard",
        "Premium",
        "Luxury"
      ]
    },
    "imageUrl": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "bio": {
      "type": "blocks",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "photos": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "specialtyDishes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::dish.dish"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "chef"
    },
    "availability": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "chef-app.weekly-schedule",
      "repeatable": true
    },
    "holidays": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "chef-app.availability",
      "repeatable": true
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "chef"
    },
    "cuisines": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::cuisine.cuisine",
      "inversedBy": "chefs"
    },
    "latitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "longitude": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "hourlyRate": {
      "type": "decimal",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "acceptsCustomMenus": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "default": false
    },
    "quote_request": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::quote-request.quote-request",
      "mappedBy": "chef"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "price_rules": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::price-rule.price-rule",
      "mappedBy": "chefs"
    }
  }
}
